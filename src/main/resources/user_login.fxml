<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userr.controllers.RegistrationController">
    <children>
        <Pane prefHeight="85.0" prefWidth="661.0" style="-fx-background-color: #20B2AA;">
            <children>
                <TextField alignment="BOTTOM_CENTER" disable="true" editable="false" layoutX="526.0" layoutY="23.0" mouseTransparent="true" prefHeight="39.0" prefWidth="135.0" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px;" text="LogIn">
                    <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                    </font>
                    <effect>
                        <Blend />
                    </effect>
                </TextField>
                <ImageView fitHeight="134.0" fitWidth="135.0" layoutX="6.0" layoutY="-24.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@logo.png" />
                    </image>
                </ImageView>
            </children>
        </Pane>
        <HBox alignment="TOP_RIGHT" layoutX="450.0" prefHeight="35.0" prefWidth="200.0">
            <children>
                <Button fx:id="minimizeField" contentDisplay="TOP" mnemonicParsing="false" onAction="#minimizeWindow" prefHeight="27.0" prefWidth="24.0" style="-fx-background-color: transparent;" text="_" textFill="#1c1c1d">
                    <font>
                        <Font name="System Bold" size="11.0" />
                    </font>
                </Button>
                <Button fx:id="closeField" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#closeWindow" prefHeight="13.0" prefWidth="25.0" style="-fx-background-color: transparent;" text="x" textAlignment="CENTER" textFill="#181819" textOverrun="CLIP">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </HBox>
        <PasswordField fx:id="passwordField" layoutX="225.0" layoutY="218.0" prefHeight="29.0" prefWidth="212.0" promptText="Password" style="-fx-background-color: transparent; -fx-border-color: #2F4F4F; -fx-border-width: 0px 0px 2px 0px;">
            <font>
                <Font name="Times New Roman" size="13.0" />
            </font>
        </PasswordField>
        <Button layoutX="561.0" layoutY="389.0" onAction="#handleRegisterAction" prefHeight="29.0" prefWidth="85.0" style="-fx-background-color: transparent;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Register" textAlignment="CENTER" textFill="DARKSLATEGRAY" textOverrun="CLIP">
            <font>
                <Font name="Times New Roman" size="16.0" />
            </font>
        </Button>
        <Pane layoutY="85.0" prefHeight="12.0" prefWidth="661.0" style="-fx-background-color: #2F4F4F;" />
        <Text fx:id="registrationMessage" fill="RED" layoutX="96.0" layoutY="439.0" wrappingWidth="394.0">
            <font>
                <Font name="Times New Roman Bold Italic" size="11.0" />
            </font>
        </Text>
        <TextField fx:id="usernameField" alignment="TOP_LEFT" layoutX="225.0" layoutY="187.0" prefHeight="29.0" prefWidth="212.0" promptText="Username" style="-fx-background-color: transparent; -fx-border-color: #2F4F4F; -fx-border-width: 0px 0px 2px 0px;">
            <font>
                <Font name="Times New Roman" size="13.0" />
            </font>
        </TextField>
        <Pane layoutY="380.0" prefHeight="85.0" prefWidth="661.0" style="-fx-background-color: #20B2AA;" />
        <Pane layoutY="368.0" prefHeight="12.0" prefWidth="661.0" style="-fx-background-color: #2F4F4F;" />
        <Button layoutX="225.0" layoutY="270.0" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-border-color: #2F4F4F; -fx-border-width: 2px 2px 2px 2px;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Log In" textFill="DARKSLATEGRAY">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
        <TextField layoutX="225.0" layoutY="337.0" opacity="0.36" style="-fx-background-color: transparent;" text="No account? please">
            <font>
                <Font name="System Italic" size="10.0" />
            </font>
        </TextField>
        <Button layoutX="323.0" layoutY="333.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="74.0" style="-fx-background-color: transparent; -fx-border-color: #2F4F4F; -fx-border-width: 2px 2px 2px 2px;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Register" textFill="DARKSLATEGRAY">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
